Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

xterm*locale: true

*customization: -color
xterm*faceName: Inconsolata\ for\ Powerline-10
xterm*faceNameDoublesize: IPAMincho
xterm*faceSize: 10

xterm*termName: xterm-256color

uxterm*faceName: Inconsolata\ for\ Powerline-10
uxterm*faceNameDoublesize: IPAMincho
uxterm*faceSize: 10

URxvt.scrollBar: false

urxvt*skipBuiltinGlyphs: true
urxvt*font:           xft:Inconsolata\ for\ Powerline:size=10,             xft:IPAMincho:size=10,             xft:Droid Sans Fallback:size=10
urxvt*imfont:         xft:Inconsolata\ for\ Powerline:size=10,             xft:IPAMincho:size=10,             xft:Droid Sans Fallback:size=10
urxvt*boldFont:       xft:Inconsolata\ for\ Powerline:bold:size=10,        xft:IPAMincho:bold:size=10,        xft:Droid Sans Fallback:bold:size=10
urxvt*italicFont:     xft:Inconsolata\ for\ Powerline:italic:size=10,      xft:IPAMincho:italic:size=10,      xft:Droid Sans Fallback:italic:size=10
urxvt*boldItalicFont: xft:Inconsolata\ for\ Powerline:bold:italic:size=10, xft:IPAMincho:bold:italic:size=10, xft:Droid Sans Fallback:bold:italic:size=10

